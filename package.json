{
  "name": "usfm-js",
  "version": "1.0.0-beta.30",
  "description": "A small library that provides functions to convert usfm to JSON and vice-versa",
  "main": "lib/index.js",
  "scripts": {
    "test": "eslint ./src ./index.js && jest",
    "fix": "eslint ./src ./index.js --fix",
    "compile": "rimraf lib && babel src/ -d lib/",
    "prepublish": "npm run compile"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage"
