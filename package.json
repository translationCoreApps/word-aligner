  "repository": {
    "type": "git",
    "url": "git+https://github.com/translationCoreApps/usfm-js.git"
  },
  "keywords": [
    "USFM",
    "Scripture",
    "JSON",
    "Bible"
  ],
  "author": "@unfoldingword",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/translationCoreApps/usfm-js/issues"
  },
  "homepage": "https://github.com/translationCoreApps/usfm-js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-jest": "^21.2.0",
    "jest": "^21.2.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "rimraf": "^2.6.2",
    "transform-runtime": "0.0.0",
    "string-punctuation-tokenizer": "0.8.0",
    "usfm-js": "1.0.0-beta.30"
